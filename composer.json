{
  "name": "abdelaziz-hassan/authentica",
  "description": "Secure Laravel client for Authentica API (OTP, Nafath, Face, Voice).",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/http": "^10.0|^11.0",
    "illuminate/validation": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "AbdelAzizHassan\\Authentica\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AbdelAzizHassan\\Authentica\\AuthenticaServiceProvider"
      ],
      "aliases": {
        "Authentica": "AbdelAzizHassan\\Authentica\\Facades\\Authentica"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^10.5"
    }
}
